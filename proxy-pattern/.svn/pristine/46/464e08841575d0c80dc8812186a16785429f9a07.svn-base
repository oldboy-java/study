package com.liuli.pattern.dynamic;

/***
 *  被代理对象：Jia老师
 * @author	刘力
 * @date	2017年9月9日上午9:16:23
 * @version 1.0
 */
public class TeacherJia implements Boy {

	public boolean date(char cupSize) {
		boolean result = false;
		if(cupSize == 'H') {
			System.out.println("这个尺寸不错，可以约会");
			result = true;
		}else {
			System.out.println("这个尺寸不满意，回去洗洗睡觉去吧");
		}
		return result;
	}

}
